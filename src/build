#! /bin/bash

# Build script generated by gb: http://go-gb.googlecode.com
# gb provides configuration-free building and distributing

echo "Build script generated by gb: http://go-gb.googlecode.com" 
if [ "$1" = "goinstall" ]; then
echo Running goinstall \
&& echo "goinstall github.com/kless/goconfig/config" \
&& goinstall github.com/kless/goconfig/config \

elif [ "$1" = "export" ]; then
echo Exporting \
&& echo "(in socketio)" && cd socketio && make && cd - > /dev/null \
&& echo "(in realtime)" && cd realtime && make $@ && cd - > /dev/null \


else
echo Building \
&& echo "(in socketio)" && cd socketio && make $1 && cd - > /dev/null \
&& echo "(in realtime)" && cd realtime && make $1 && cd - > /dev/null \

fi

# The makefiles above are invoked in topological dependence order
